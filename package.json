{
  "name": "brightspace-integration",
  "description": "Integration project for components consumed by Brightspace.",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "build": "npm run build:legacy && npm run build:common",
    "build:common": "rollup -c ./rollup/rollup.config.js",
    "build:legacy": "cross-env NODE_OPTIONS=--max_old_space_size=4096 polymer build && babel-external-helpers > ./build/babel-helpers.js && cpy \"node_modules/@polymer/esm-amd-loader/lib/esm-amd-loader.min.js\" \"build\" && node ./cli/rename-define.js \"./build/esm-amd-loader.min.js\" \"./build/es5-bundled/**/*.js\" \"./build/es6-bundled/**/*.js\"",
    "build:lang": "node ./oslo/generate-monolith-xml",
    "build:serge-mapping": "node ./oslo/generate-serge-mapping",
    "build:unbundled": "rollup -c ./rollup/rollup-wc.config.js",
    "lint": "eslint *.js web-components/** js/** rollup/** test/** cli/** oslo/*.js",
    "publish:autotag": "node ./cli/autotag.js",
    "publish:cdn": "frau-publisher -m lib -t bsi -f \"./build/**/*\" --versionvar TRAVIS_TAG",
    "preserve": "npm run build",
    "serve": "npm run start:prod",
    "start": "npm run start:legacy",
    "start:dev": "concurrently --kill-others \"es-dev-server --config es-dev-server-dev.config.js\" \"rollup -w -c ./rollup/rollup.config.js\"",
    "start:legacy": "cross-env es-dev-server --config=./es-dev-server-legacy.config.js --compatibility auto --watch",
    "start:prod": "es-dev-server --config es-dev-server-prod.config.js",
    "test": "npm run lint && npm run test:unit",
    "test:ci": "npm run test && npm run test:package-lock",
    "test:package-lock": "node test-package-lock.js",
    "test:unit": "mocha --require @babel/register --require regenerator-runtime/runtime"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Brightspace/brightspace-integration.git"
  },
  "keywords": [],
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Brightspace/brightspace-integration/issues"
  },
  "homepage": "https://github.com/Brightspace/brightspace-integration",
  "devDependencies": {
    "@babel/cli": "^7",
    "@babel/core": "^7",
    "@babel/preset-env": "^7",
    "@babel/register": "^7",
    "@brightspace-ui-labs/user-feedback": "^1",
    "@brightspace-ui/core": "^1.49.0",
    "@brightspace-ui/intl": "^3",
    "@d2l/switch": "Brightspace/d2l-switch.git#semver:^3",
    "@koa/cors": "^3",
    "@octokit/rest": "^17",
    "@open-wc/building-rollup": "^1",
    "@polymer/esm-amd-loader": "^1",
    "@polymer/polymer": "^3",
    "@rollup/plugin-commonjs": "^11",
    "@webcomponents/webcomponentsjs": "^2",
    "autoprefixer": "^9",
    "babel-eslint": "^10",
    "chai": "^4",
    "chalk": "^2",
    "concurrently": "^5",
    "cpy-cli": "^3",
    "cross-env": "^7",
    "d2l-activities": "BrightspaceHypermediaComponents/activities.git#semver:^3",
    "d2l-activity-alignments": "Brightspace/d2l-activity-alignments.git#semver:^2",
    "d2l-activity-exemptions": "Brightspace/d2l-activity-exemptions#semver:^2",
    "d2l-awards-leaderboard-ui": "Brightspace/awards-leaderboard-ui#semver:^1",
    "d2l-button-group": "BrightspaceUI/button-group#semver:^3",
    "d2l-consistent-evaluation": "BrightspaceHypermediaComponents/consistent-evaluation#v0.0.22",
    "d2l-content-store": "Brightspace/d2l-content-store#semver:^0",
    "d2l-cpd": "Brightspace/continuous-professional-development#semver:^1",
    "d2l-fetch": "Brightspace/d2l-fetch.git#semver:^2",
    "d2l-fetch-auth": "^1",
    "d2l-fetch-dedupe": "^1",
    "d2l-fetch-simple-cache": "^1",
    "d2l-image-banner-overlay": "Brightspace/d2l-image-banner-overlay#semver:^4",
    "d2l-my-courses": "Brightspace/d2l-my-courses-ui#semver:^11",
    "d2l-my-dashboards": "github:Brightspace/d2l-my-dashboards-ui#semver:^3",
    "d2l-navigation": "BrightspaceUI/navigation#semver:^4",
    "d2l-opt-in-flyout-webcomponent": "Brightspace/d2l-opt-in-flyout-webcomponent#semver:^2",
    "d2l-organizations": "BrightspaceHypermediaComponents/organizations#semver:^5",
    "d2l-outcomes-level-of-achievements": "Brightspace/outcomes-level-of-achievement-ui.git#semver:^2",
    "d2l-outcomes-user-progress": "Brightspace/d2l-outcomes-user-progress.git#semver:^1",
